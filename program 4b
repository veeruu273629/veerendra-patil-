#include <iostream>
using namespace std;
void swapValue(int a, int b) {
int temp = a;
a = b;
b = temp;
cout << "After swap inside function: a=" << a << ", b=" << b << endl;
}
int main() {
int a,b;
cout << "Enter two numbers: ";
cin >> a >> b;
swapValue(a,b);
cout << "After function call: a=" << a << ", b=" << b << endl;
return 0;
}
